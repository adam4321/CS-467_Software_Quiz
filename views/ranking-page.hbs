{{!-- Settings view - using express handlebars --}}

<title>Quiz Soft | Settings</title>

{{!-- Container for the dashboard --}}
<div id="top-row-container">
    <div id="dashboard-container" class="mdl-card mdl-shadow--2dp">

        <div class="mdl-card__title mdl-color-text--white dashboard-top-bar">
            <h2 class="mdl-card__title-text">Ranking</h2>
        </div>

            <div class="mdl-card__supporting-text mdl-color-text--grey-600">
                <h3 style="font-size: large; font-weight: bold;">Sample Ranking Below:</h3>
                <p style="font-size: large; font-weight: bold;">For the position: {{title}}</p>
                <p style="font-size: large; font-weight: bold;">Candidate: {{cand_name}}</p>
                <p style="font-size: large; font-weight: bold;">Score: {{score}}</p>

            </div>

        </div>
    </div>

</div>
